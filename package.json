{
  "name": "double-agent",
  "version": "1.0.1",
  "private": true,
  "description": "Double agent is a detection system for when an http request or browser interaction is not the user agent it says it is",
  "scripts": {
    "build": "tsc --build",
    "watch": "tsc -w",
    "docs": "node docs/generator",
    "start": "yarn workspace @double-agent/collect-controller start",
    "prod": "yarn workspace @double-agent/collect-controller prod",
    "clean": "tsc -b --clean",
    "lint": "eslint --cache ./"
  },
  "author": "Data Liberation Foundation",
  "license": "ISC",
  "engines": {
    "node": ">=12.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "cookie": "^0.4.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@types/cookie": "^0.3.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^12.7.11",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-monorepo-cop": "^1.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "p-queue": "^6.2.1",
    "prettier": "^2.3.0",
    "pretty-quick": "^1.8.0",
    "request": "^2.88.2",
    "source-map-support": "^0.5.16",
    "typescript": "^4.2.4",
    "yamlparser": "0.0.x"
  },
  "workspaces": {
    "packages": [
      "analyze",
      "analyze/plugins/*",
      "collect",
      "collect/plugins/*",
      "collect-controller",
      "config",
      "external-data",
      "docs",
      "real-user-agents",
      "tls-server",
      "runner",
      "stacks",
      "../ulixee/hero/build/*",
      "../ulixee/hero/build/plugins/*"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
